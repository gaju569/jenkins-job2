pipeline {

  agent any
   stages {
   
      stage('build') {
         steps {
          echo "building tomcat-application.."
         }
      }    

      stage('test') {
         steps {
          echo "testing tomcat-application..."
         }
      }
   
      stage('deploy')
        steps {
         echo "deploying tomcat-application..."
        } 
      } 
   } 
}     
 


